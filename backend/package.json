{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && npx tsc && tsc-alias",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "npx tsc -w & tsc-alias -w & nodemon --ignore 'temp-cache/*' dist/index.js",
    "predev": "npm run build"
  },
  "nodemonConfig": {
    "ignore": [
      "temp-cache/*",
      "*.json"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "socket.io": "^4.8.1"
  }
}
